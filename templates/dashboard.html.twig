<script>
    var route_name = "{{ app.request.attributes.get('_route') }}";
    var path = "{{ app.request.pathInfo }}";
    if(route_name == "app_profil" || route_name == "fos_user_change_password") {
        $("#profil").addClass('active-submenu');
        $("#profil_a").addClass('active');

    }
    if(route_name == "dashboard" ) {
        $("#dashboad").addClass('active-submenu');
        $("#dashboad_a").addClass('active');

    }


    if(route_name =='admin_project_index' || route_name =='admin_project_new'|| route_name =='admin_project_edit')
    {


        $("#project").addClass('active-submenu');
        $("#project_a").addClass('active');
        $('#project_new').css({
            'background-color': 'rgb(223 147 152)',
            'color' : 'white',

        });
        $('#project_new_a').css({
            'color' : 'white',

        });


    }





</script>
<!-- Snackbar // documentation: https://www.polonel.com/snackbar/ -->
<script>
    // Snackbar for user status switcher
    $('#snackbar-user-status label').click(function() {
        Snackbar.show({
            text: 'Your status has been changed!',
            pos: 'bottom-center',
            showAction: false,
            actionText: "Dismiss",
            duration: 3000,
            textColor: '#fff',
            backgroundColor: '#383838'
        });
    });
</script>

<!-- Chart.js // documentation: http://www.chartjs.org/docs/latest/ -->
<script src="{{ asset('front/js/chart.min.js') }}"></script>




